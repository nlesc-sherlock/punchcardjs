{
  "name": "punchcards",
  "version": "1.0.0",
  "description": "Punchcard visualization using TypeScript and D3.js",
  "main": "./dist/punchcards.js",
  "scripts": {
    "postinstall": "typings install",
    "lint": "npm run tslint && npm run jshint && npm run jslint && npm run csslint",
    "tslint": "tslint --config tslint.json ./src/**/*.ts",
    "jslint": "jslint test/*.js",
    "jshint": "jshint test/*.js",
    "csslint": "csslint src/",
    "pretsc": "npm run tslint",
    "tsc": "tsc --project .",
    "predist": "trash dist/ && mkdirp dist",
    "dist": "browserify --no-bundle-external --debug build/punchcards.js --outfile build/punchcards-with-sourcemaps.js --standalone punchcards",
    "postdist": "npm run exorcist && npm run uglify",
    "exorcist":"<build/punchcards-with-sourcemaps.js exorcist dist/punchcards.js.map > dist/punchcards.js",
    "uglify": "uglifyjs dist/punchcards.js --output dist/punchcards.min.js --source-map punchcards.min.js.map --in-source-map dist/punchcards.js.map",
    "pretest": "npm run jshint && npm run jslint",
    "test": "karma start karma.conf.js",
    "precover": "trash sites/coverage/ && npm run tsc && npm run dist",
    "cover": "npm run test && npm run remap-istanbul-json && echo 'TODO npm run remap-istanbul-html'",
    "remap-istanbul-json": "remap-istanbul --input sites/coverage/coverage.json --output sites/coverage/coverage-remapped.json",
    "remap-istanbul-html": "remap-istanbul --input sites/coverage/coverage-remapped.json --output sites/coverage/remapped --type html --exclude node_modules",
    "pretsdoc": "npm run clean-tsdoc && mkdirp sites/tsdoc",
    "tsdoc": "typedoc --out sites/tsdoc/ .",
    "clean": "npm run clean-build & npm run clean-dist & npm run clean-sites",
    "clean-build": "trash build/",
    "clean-dist": "trash dist/",
    "clean-sites": "npm run clean-cover & npm run clean-demo & npm run clean-tsdoc",
    "clean-cover": "trash sites/coverage/",
    "clean-demo": "trash sites/demo/*.js !sites/demo/main-script.js sites/demo/*.css sites/demo/bootstrap-css sites/demo/fonts",
    "clean-tsdoc": "trash sites/tsdoc/",
    "clean-downloaded": "trash typings/ node_modules/",
    "purge": "npm run clean & npm run clean-downloaded",
    "copy-sites-demo-deps": "npm run copy-sites-demo-js && npm run copy-sites-demo-css && npm run copy-sites-demo-fonts",
    "copy-sites-demo-css": "cp src/*.css sites/demo/ && cp node_modules/bootstrap/dist/css/bootstrap.min.css node_modules/bootstrap/dist/css/bootstrap-theme.min.css sites/demo/bootstrap-css/",
    "copy-sites-demo-fonts": "cp node_modules/bootstrap/dist/fonts/* sites/demo/fonts/",
    "copy-sites-demo-js": "cp node_modules/crossfilter/crossfilter.min.js node_modules/moment/min/moment.min.js node_modules/d3/d3.min.js sites/demo/",
    "predemo": "npm run clean-demo && mkdirp sites/demo/fonts && mkdirp sites/demo/bootstrap-css && npm run copy-sites-demo-deps",
    "demo": "npm run tsc && npm run dist",
    "postdemo": "cp dist/punchcards.js dist/punchcards.js.map dist/punchcards.min.js dist/punchcards.min.js.map sites/demo/ && tsc --target es5 sites/demo/dataloader.ts --outDir sites/demo",
    "everything": "npm run demo && npm run tsdoc && npm run cover"
  },
  "keywords": [
    "typescript",
    "punchcard",
    "visualization"
  ],
  "author": "Johan Hidding, Mateusz Kuzak, Jurriaan H. Spaaks, Lourens Veen, Faruk Diblen",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/nlesc-sherlock/punchcardjs.git"
  },
  "devDependencies": {
    "bootstrap": "^3.3.7",
    "bower": "^1.7.9",
    "browserify": "^13.0.1",
    "browserify-shim": "^3.8.12",
    "crossfilter": "^1.3.12",
    "csslint": "^1.0.2",
    "d3": "^3.5.17",
    "exorcist": "^0.4.0",
    "istanbul": "^0.4.4",
    "jasmine-core": "^2.4.1",
    "jshint": "^2.9.3",
    "jslint": "^0.10.3",
    "karma": "^1.2.0",
    "karma-coverage": "^1.1.1",
    "karma-fixture": "^0.2.6",
    "karma-html2js-preprocessor": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-json-fixtures-preprocessor": "0.0.6",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-spec-reporter": "0.0.26",
    "mkdirp": "^0.5.1",
    "moment": "^2.14.1",
    "remap-istanbul": "^0.6.4",
    "trash-cli": "^1.3.0",
    "tslint": "^3.9.0",
    "typedoc": "^0.4.1",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "uglify-js": "^2.7.3",
    "uglifyjs": "^2.4.10"
  },
  "dependencies": {},
  "browser": {
    "crossfilter": "node_modules/crossfilter/crossfilter.js",
    "d3": "node_modules/d3/d3.js",
    "moment": "node_modules/moment/moment.js"
  },
  "browserify-shim": {
    "crossfilter": "global:crossfilter",
    "d3": "global:d3",
    "moment": "global:moment"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  }
}
